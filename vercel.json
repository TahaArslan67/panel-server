{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate=59"
      }
    }
  ],
  "regions": ["fra1"],
  "functions": {
    "src/index.ts": {
      "memory": 1024,
      "maxDuration": 3
    }
  }
} 